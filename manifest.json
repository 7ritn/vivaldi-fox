{
  "manifest_version": 2,
  "name": "VivaldiFox",
  "version": "2.0",
  "description": "Delicious themes",
  "permissions": ["theme", "tabs", "<all_urls>", "storage"],
  "background": {
    "scripts": [
      "shared/settings.js",
      "background/color.js",
      "background/addon-state.js",
      "background/color-utils.js",
      "background/theme.js", 
      "background/background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html"
  },
  "browser_action": {
    "default_title": "VivaldiFox"
  },
  "author": "Tim Nguyen",
  "applications": {
    "gecko": {
      "id": "vivaldi-fox@tim-addons.com",
      "strict_min_version": "57.0a1"
    }
  }
}
